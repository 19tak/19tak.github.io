<h2 id="ì½”ë“œ">ì½”ë“œ</h2>
<div class="language-plaintext highlighter-rouge"><div class="highlight"><pre class="highlight"><code># setting
rm(list=ls())
getwd()
setwd("D:\\tak\\r\\doit_book\\ml\\machinelearning")

# library
library(ggplot2)

data(iris)
# ê·¸ë˜í”„ë¥¼ gì— í• ë‹¹ 
g &lt;- ggplot(data = iris, mapping = aes(x = Sepal.Length, y = Sepal.Width)) + 
  geom_point(colour = c("purple", "blue", "green")[iris$Species], 
             pch = c(0, 2, 20)[iris$Species], 
             size = c(1, 1.5, 2)[iris$Species])

g + 
  annotate(geom = "text", 
           x = iris$Sepal.Length, 
           y = iris$Sepal.Width, 
           label = rownames(iris), 
           colour = "brown", 
           alpha = 0.7, size = 3, hjust = 0.5, vjust = -1) +
  labs(title = "ggplot", subtitle = "iris Sepal.Length vs Sepal.Width", 
       caption = "í•œì •íƒ", x = "Sepal.Length", y = "Sepal.Width")
</code></pre></div></div>
